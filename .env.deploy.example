# AWS Configuration
AWS_ACCOUNT_ID=123456789012
AWS_REGION=us-east-1

# Domain Configuration
FRONTEND_DOMAIN=your-domain.com
API_DOMAIN=api.your-domain.com

# Stack and Bucket Names
BACKEND_STACK_NAME=my-app-backend-prod
FRONTEND_BUCKET_PREFIX=my-app-frontend

# CloudFront Distribution IDs (obtained after deployment)
# Run: aws cloudformation describe-stacks --stack-name <BACKEND_STACK_NAME> --query "Stacks[0].Outputs"
CLOUDFRONT_DISTRIBUTION_ID=

# Optional: Override default AWS profile
# AWS_PROFILE=default
